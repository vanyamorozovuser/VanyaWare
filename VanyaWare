game.Players.LocalPlayer.Chatted:Connect(function(msg)
if msg == "$s load" then
loadstring(game:HttpGet("https://raw.githubusercontent.com/vanyamorozovuser/tooolsource/main/tooolsource"))()
game.StarterGui:SetCore("SendNotification", {
     Title = 'you use comand $s load';
     Text = 'da hood script loaded. key binds: (x) antilock, (i) target, (p) aimviewer.';
     Icon = 'rbxassetid://14821554493';
     Duration = "2";
})
end
end)
